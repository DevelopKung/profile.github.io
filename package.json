{
  "name": "My-ProFile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "gitCommit": "git add . && git commit -m 'deploy'",
    "pushDir": "nuxt generate && npm run gitCommit && push-dir --dir=dist --branch=gh-pages --cleanup",
    "pushMaster": "npm run gitCommit && git branch -M master && git push -u origin master",
    "deploy": "npm run pushDir"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/moment": "^1.6.1",
    "animate.css": "^4.1.1",
    "dotenv": "^10.0.0",
    "nuxt": "^2.15.8",
    "typed.js": "^2.0.12",
    "vue": "^2.6.14",
    "vuetify": "^2.6.1",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@nuxtjs/vuetify": "^1.12.3",
    "push-dir": "^0.4.1"
  }
}
